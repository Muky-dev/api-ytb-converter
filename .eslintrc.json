// Setup eslint to express
{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "no-undef": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-continue": 0,
    "no-shadow": 0,
    "no-prototype-builtins": 0,
    "no-else-return": 0
  }
}
